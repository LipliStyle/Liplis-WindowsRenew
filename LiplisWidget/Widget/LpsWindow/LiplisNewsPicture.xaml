<Window x:Class="Liplis.Widget.LpsWindow.LiplisNewsPicture"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Liplis.Widget.LpsWindow"
        mc:Ignorable="d"
        AllowsTransparency="True"
        Background="{x:Null}"
        WindowStyle="None"
        Title="LiplisNewsPicture" Height="100" Width="100">
    <DockPanel>
        <Image x:Name="image" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </DockPanel>
</Window>
